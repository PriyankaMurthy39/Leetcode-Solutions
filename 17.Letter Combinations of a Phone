class Solution(object):
    def letterCombinations(self, digits):
        if digits=="":
            return[]
        phone = {
            "2": "abc",
            "3": "def",
            "4": "ghi",
            "5": "jkl",
            "6": "mno",
            "7": "pqrs",
            "8": "tuv",
            "9": "wxyz"
        }

        result=[""]
        for d in digits:
          temp=[]
          for combo in result:
            for ch in phone[d]:
                temp.append(combo+ch)
          result=temp
        return result

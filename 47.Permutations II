class Solution(object):
    def permuteUnique(self, nums):
        result=[]
        nums.sort()
        def backtrack(path,remaining):
            if not remaining:
                result.append(path)
                return 
            for i in range(len(remaining)):
                if i>0 and remaining[i]==remaining[i-1]:
                    continue
                backtrack(
                    path+[remaining[i]],remaining[:i]+remaining[i+1:]
            )
        backtrack([],nums)
        return result
        

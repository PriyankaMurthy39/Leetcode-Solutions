class Solution(object):
    def isValidSudoku(self, board):
       seen=set()
       for r in range(9):
         for c in range(9):
            val=board[r][c]
            if val==".":
                continue
            row_check=("row",r,val)
            col_check=("col",c,val)
            box_check=("box",val, r//3, c//3)
            if row_check in seen or col_check in seen or box_check in seen:
                return False
            seen.add(row_check)
            seen.add(col_check)
            seen.add(box_check)
       return True

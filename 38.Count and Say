class Solution(object):
    def countAndSay(self, n):
        s="1"
        for _ in range(n-1):
            new=""
            count=1
            for i in range(1,len(s)):
                if s[i]==s[i-1]:
                    count+=1
                else:
                    new+=str(count)+s[i-1]
                    count=1
            new+=str(count)+s[-1]
            s=new
        return s
